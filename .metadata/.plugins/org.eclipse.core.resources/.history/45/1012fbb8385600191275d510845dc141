#include <GPIO.h>



enum PortDirection_t {
			INPUT = 0,
			OUTPUT=1
		};



namespace GPIO {

uint8_t _id;
uint8_t _bit;

GPIO::GPIO(uint8_t id,  PortDirection_t dir ){

	switch (id) {
	case 2: //PE4

	break;
	case 3: //PE5
		this._id = id;
		_
	break;
	case 4: //PG5
	break;
	case 5: //PE3
	break;
	case 6: //PH3
	break;
	case 7: //PH4
	break;
	case 8: //PH5
	break;
	case 9: //PH6
	break;

	}

}



}



