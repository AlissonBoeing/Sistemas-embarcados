#ifndef TIMEOUT_H_
#define TIMEOUT_H_
#include <avr/io.h>
#include <avr/interrupt.h>


typedef void (*CALLBACK_t)(void);

class Timeout {
	public:


		uint32_t _interval;
	private:
		bool _event;
		bool _enable;
		uint32_t counter;




};


#endif /* TIMEOUT_H_ */
